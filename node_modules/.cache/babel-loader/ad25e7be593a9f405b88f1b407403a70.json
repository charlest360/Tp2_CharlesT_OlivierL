,2BAgFA;AACA,UAAA,KAAA,KAAA,IAAA,EAAA,EAAA;AACA,aAAA,gBAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,OAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA,OAHA,MAIA,IAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA,IAAA,KAAA,KAAA,CAAA,MAAA,GAAA,EAAA,EAAA;AACA,aAAA,gBAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,OAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA,KA1FA;AA2FA,IAAA,iBA3FA,+BA2FA;AACA,UAAA,KAAA,SAAA,IAAA,EAAA,EAAA;AACA,aAAA,gBAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,WAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA,OAHA,MAIA,IAAA,KAAA,SAAA,CAAA,MAAA,GAAA,CAAA,IAAA,KAAA,SAAA,CAAA,MAAA,GAAA,EAAA,EAAA;AACA,aAAA,gBAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,WAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA,KArGA;AAsGA,IAAA,gBAtGA,8BAsGA;AACA,UAAA,KAAA,QAAA,IAAA,EAAA,EAAA;AACA,aAAA,gBAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,UAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA,OAHA,MAIA,IAAA,KAAA,QAAA,CAAA,MAAA,GAAA,CAAA,IAAA,KAAA,QAAA,CAAA,MAAA,GAAA,EAAA,EAAA;AACA,aAAA,gBAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,UAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA,eAAA,KAAA;AACA;;AACA,aAAA,IAAA;AACA;AAhHA;AAxBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"createAccountForm\">\r\n        <div v-if=\"error != null\">\r\n             <span>Erreur : {{error}}</span>\r\n        </div>\r\n        <div v-if=\"response != null\">\r\n            <span> Account edited!</span>\r\n        </div>\r\n        <form @submit.prevent=\"validateAndSend\" v-else>\r\n            <ul>           \r\n                \r\n                <li>\r\n                    <label class=\" form__label\" for=\"registrationForm_Last_Name\">Last name :</label>\r\n                    <input v-model=\"lastName\" type=\"text\" class=\"form__control \" id=\"registrationForm_Last_Name\" name=\"lastName\" >\r\n                </li>\r\n                <div v-if=\"validationErrors.length >= 1 && validationErrors[0].type == 'lastName' \"> \r\n                    <span style=\"color:red;\">{{validationErrors[0].message}} </span>\r\n                </div>\r\n                \r\n                <li>\r\n                    <label class=\" form__label\" for=\"registrationForm_First_Name\">First name :</label>\r\n                    <input v-model=\"firstName\" type=\"text\" class=\"form__control \" id=\"registrationForm_First_Name\" name=\"firstName\" >\r\n                </li>\r\n                <div v-if=\"validationErrors.length >= 1 && validationErrors[0].type == 'firstName' \"> \r\n                    <span style=\"color:red;\">{{validationErrors[0].message}} </span>\r\n                </div>\r\n                \r\n                <li>\r\n                    <label class=\" form__label\" for=\"registrationForm_Password\">Password :</label>\r\n                    <input v-model=\"password\" type=\"password\" class=\"form__control \" id=\"registrationForm_Password\" name=\"Password\" autocomplete=\"off\">\r\n                </li>\r\n                <div v-if=\"validationErrors.length >= 1 && validationErrors[0].type == 'password' \"> \r\n                    <span style=\"color:red;\">{{validationErrors[0].message}} </span>\r\n                </div>\r\n\r\n\r\n                <li>\r\n                    <label class=\" form__label\" for=\"registrationForm_ConfirmPassword\">Confirm Password :</label>\r\n                    <input v-model=\"confirmPassword\" type=\"password\" class=\"form__control \" id=\"registrationForm_ConfirmPassword\" name=\"Password\" autocomplete=\"off\">\r\n                </li>\r\n                <div v-if=\"validationErrors.length >= 1 && validationErrors[0].type == 'confirmPassword' \"> \r\n                    <span style=\"color:red;\">{{validationErrors[0].message}} </span>\r\n                </div>\r\n                \r\n                \r\n                <li>\r\n                    <label class=\" form__label\" for=\"registrationForm_Email\">Email :</label>\r\n                    <input v-model=\"email\" type=\"text\" class=\"form__control \" id=\"registrationForm_Email\" name=\"Email\">\r\n                </li>\r\n                <div v-if=\"validationErrors.length >= 1 && validationErrors[0].type == 'email' \"> \r\n                    <span style=\"color:red;\">{{validationErrors[0].message}} </span>\r\n                </div>\r\n                \r\n\r\n         \r\n                \r\n                <li>\r\n                    <button  id=\"submitButton\" >\r\n                        Edit account\r\n                    </button>\r\n                </li>\r\n\r\n            </ul>\r\n        </form>\r\n        \r\n        \r\n        \r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport FilmService from '@/services/FilmService.js';\r\n    export default {\r\n     \r\n        data() {\r\n            return {\r\n                password: '',\r\n                confirmPassword: '',\r\n                email: '',\r\n                lastName: '',\r\n                firstName: '',\r\n                response: null,\r\n                error: null,\r\n                validationErrors: [],\r\n                success: null,\r\n                userInfos : null,\r\n                role_id : null,\r\n                login : null,\r\n            }\r\n        },\r\n        created () {\r\n            //this.success = FilmService.setToken();\r\n            this.getUserInfos();\r\n           \r\n        },\r\n              \r\n        methods: {\r\n            getUserInfos(){\r\n                FilmService.getUserInfos(localStorage.getItem('user_id'),localStorage.getItem('token'))\r\n                    .then(response => {\r\n                    this.userInfos = response.data.user;\r\n                    this.email = this.userInfos.email;\r\n                    this.lastName = this.userInfos.last_name;\r\n                    this.firstName = this.userInfos.first_name;\r\n                    this.login = this.userInfos.login;\r\n                    this.role_id = this.userInfos.role_id\r\n                    })\r\n                    .catch(error => {\r\n                    this.error = error;\r\n                    });\r\n            },\r\n            validateAndSend(){\r\n                this.success = this.validateData();\r\n                if( this.success == true){\r\n                    let data = {\r\n                        password : this.password,\r\n                        email : this.email,\r\n                        last_name : this.lastName,\r\n                        first_name : this.firstName,\r\n                        role_id : this.role_id,\r\n                        login : this.login\r\n                    }\r\n                    FilmService.editUser(localStorage.getItem('user_id'),data,localStorage.getItem('token'))\r\n                    .then(response => {\r\n                    this.response = response.data;\r\n                    })\r\n                    .catch(error => {\r\n                    this.error = error;\r\n                    });\r\n\r\n                    FilmService.editUserPassword(localStorage.getItem('user_id'),data,localStorage.getItem('token'))\r\n                    .then(response => {\r\n                    this.response = response.data;\r\n                    })\r\n                    .catch(error => {\r\n                    this.error = error;\r\n                    });\r\n                }\r\n            },\r\n            validateData() {\r\n                this.validationErrors = [];\r\n                if (this.validatePassword() ==true &&\r\n                this.validateConfirmPassword() &&\r\n                this.validateEmail() == true && \r\n                this.validateLastName()== true &&\r\n                this.validateFirstName() == true\r\n                ) \r\n                {\r\n                    return true;\r\n                }\r\n                else{\r\n                    return false;\r\n                }\r\n            },\r\n            validatePassword(){\r\n                if(this.password == ''){\r\n                    this.validationErrors.push({type:'password',message:'Password field is required'});\r\n                    return false;\r\n                }\r\n                else if(this.password.length < 5 || this.password.length > 50) {\r\n                    this.validationErrors.push({type:'password',message:'Password field must be between 5 and 50 characters long'});\r\n                    return false;\r\n                }\r\n                return true;\r\n            },\r\n            validateConfirmPassword(){\r\n                if(this.confirmPassword == ''){\r\n                    this.validationErrors.push({type:'confirmPassword',message:'You must confirm your password'});\r\n                    return false;\r\n                }\r\n                else if(this.confirmPassword != this.password) {\r\n                    this.validationErrors.push({type:'confirmPassword',message:'Passwords are not identical'});\r\n                    return false;\r\n                }\r\n                return true;\r\n            },\r\n            validateEmail(){\r\n                if(this.email == ''){\r\n                    this.validationErrors.push({type:'email',message:'Email field is required'});\r\n                    return false;\r\n                }\r\n                else if(this.email.length < 8 || this.email.length > 50) {\r\n                    this.validationErrors.push({type:'email',message:'Please enter a valid email address '});\r\n                    return false;\r\n                }\r\n                return true;\r\n            },\r\n            validateFirstName(){\r\n                if(this.firstName == ''){\r\n                    this.validationErrors.push({type:'firstName',message:'First name field is required'});\r\n                    return false;\r\n                }\r\n                else if(this.firstName.length < 2 || this.firstName.length > 50) {\r\n                    this.validationErrors.push({type:'firstName',message:'First name field must be between 2 and 50 characters '});\r\n                    return false;\r\n                }\r\n                return true;\r\n            },\r\n            validateLastName(){\r\n                if(this.lastName == ''){\r\n                    this.validationErrors.push({type:'lastName',message:'Last name field is required'});\r\n                    return false;\r\n                }\r\n                else if(this.lastName.length < 2 || this.lastName.length > 50) {\r\n                    this.validationErrors.push({type:'LastName',message:'Last name field must be between 2 and 50 characters '});\r\n                    return false;\r\n                }\r\n                return true;\r\n            },\r\n        } \r\n    }\r\n    \r\n</script>\r\n\r\n<style scoped>\r\n    .form__label {\r\n        float: left;\r\n        width: 25%;\r\n        text-align: left;\r\n        padding-bottom: 5px;\r\n        font-size: 20px;\r\n        color: black;\r\n    }\r\n    li {\r\n        margin-top: 20px;\r\n\r\n        \r\n\r\n    }\r\n    .form__control{\r\n        display: inline-block;\r\n        width: 70%;\r\n        padding-bottom: 5px;\r\n        padding-top: 5px;\r\n    }\r\n    ul {\r\n        list-style-type: none;\r\n    }\r\n    .createAccountForm{\r\n        width: 50%;\r\n        background-color: #fcdc6d;\r\n        display: inline-block;\r\n        margin: auto;\r\n    }\r\n    #submitButton {\r\n        width: 50%;\r\n        font-size: 20px;\r\n        padding-top: 10px;\r\n        padding-bottom: 10px;\r\n        font-weight: bold;\r\n        color: #E6B91E;\r\n        background-color: black;\r\n    }\r\n    .accountContent{\r\n        color: black;\r\n    }\r\n    .hidden {\r\n        display: none;\r\n    }\r\n    .error {\r\n        float: left;\r\n    }\r\n    \r\n</style>"],"sourceRoot":"src/components"}]}